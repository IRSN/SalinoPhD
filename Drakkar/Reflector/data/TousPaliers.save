#!/bin/sh
#
if [ $# = 0 ]
   then
   echo "usage: *.save directory" 1>&2
   exit 1
fi

System=`uname -s`
Sysx="`echo $System | cut -b -6`"
if [ $Sysx = "CYGWIN" ]; then
   MACH=`uname -o | sed 's/ /_/g'`
elif [ $Sysx = "AIX" ]; then
   MACH=`uname -s | sed 's/ /_/g'`
else
   MACH=`uname -sm | sed 's/ /_/g'`
fi

mv *Refl_* $1/../save/
